{
  "parser": "babel-eslint",
  "extends": ["prettier"],
  "plugins": ["prettier", "react", "react-hooks"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "default-param-last": "off",
    "padding-line-between-statements": "off",
    "no-eq-null": "off",
    "eqeqeq": [
      "error",
      "always",
      {
        "null": "ignore"
      }
    ],
    "prettier/prettier": "error"
  }
}
